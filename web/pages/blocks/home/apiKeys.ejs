<main class="container page-tab pt-3" id="tab-apiKeys">
    <div class="dark" id="apis">
        <%  Object.keys(define['API Keys'].blocks).forEach(function(blockKey) { -%>
            <%- include('drawBlock', {
                  theBlock: define['API Keys'].blocks[blockKey]
                }) %>
          <%  }) -%>
          <div style="margin-bottom: 50px;"></div>
          <div class="sticky-bar p-3">
              <button type="button" class="btn btn-sm btn-primary reset-form" style="min-width: 150px"><i class="fa fa-refresh"></i> <%- lang.Clear %></button>
              <button type="button" class="btn btn-sm btn-success submit" style="min-width: 150px"><i class="fa fa-check"></i> <%- lang.Save %></button>
          </div>
    </div>
</main>
